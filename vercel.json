{
    "version": 2,
    "builds": [
      {
        "src": "./backend/server",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "./backend/(.*)",
        "dest": "/"
      }
    ],
    "rewrites": [
        { 
        "source": "/api/(.*)", 
        "destination": "/api" 
      }]
  } 
